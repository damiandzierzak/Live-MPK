<?xml version='1.0' encoding='utf-8'?>

<widget id="Live-MPK" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Live-MPK</name>
    <description>Live-MPK: App shows live data from Traffic Tram Supervision System in Cracow</description>
    <author email="johnbrown@gmail.com" href="http://mypersonalwebsite.com">
        Damian Dzierzak Mateusz Jaskula
    </author>
    <platform name="android" />
    <plugin name="cordova-plugin-device" spec="~1.1.4" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.1" />
    <plugin name="cordova-plugin-http" spec="~1.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <icon src="res/icon/android/ic_launcher_hdpi.png" platform="android" qualifier="hdpi" />
    <icon src="res/icon/android/ic_launcher_mdpi.png" platform="android" qualifier="mdpi" />
    <icon src="res/icon/android/ic_launcher_xdpi.png" platform="android" qualifier="xhdpi" />
    <icon src="res/icon/android/ic_launcher_xxdpi.png" platform="android" qualifier="xxhdpi" />
    <icon src="res/icon/android/ic_launcher_xxxdpi.png" platform="android" qualifier="xxxhdpi" />
    <splash density="ldpi" src="res/screens/android/screen-ldpi-portrait.png"/>
    <splash density="mdpi" src="res/screens/android/screen-mdpi-portrait.png"/>
    <splash density="hdpi" src="res/screens/android/screen-hdpi-portrait.png"/>
    <splash density="xhdpi" src="res/screens/android/screen-xhdpi-portrait.png"/>
    <splash density="xxhdpi" src="res/screens/android/screen-xxhdpi-portrait.png"/>
    <splash density="xxxhdpi" src="res/screens/android/screen-xxxhdpi-portrait.png"/>

    <allow-navigation href="*" />
</widget>